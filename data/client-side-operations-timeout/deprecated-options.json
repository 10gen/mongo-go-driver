{
  "description": "operations ignore maxTimeMS and maxCommitTimeMS options if timeoutMS is set",
  "schemaVersion": "1.9",
  "runOnRequirements": [
    {
      "minServerVersion": "4.2",
      "topologies": [
        "replicaset",
        "sharded-replicaset"
      ]
    }
  ],
  "initialData": [
    {
      "collectionName": "coll",
      "databaseName": "test",
      "documents": []
